{
  "name": "date",
  "version": "0.1.10",
  "description": "Date controls and utils",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.babel.js",
    "demo": "react-scripts start",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "flow": "flow",
    "lint": "eslint *.js src/lib",
    "docs": "jsdoc -c jsdoc.json",
    "docs:explain": "jsdoc -X -c jsdoc.json"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/date.git"
  },
  "keywords": [
    "react",
    "controls",
    "date",
    "moment"
  ],
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.49.1",
    "identity-obj-proxy": "^3.0.0",
    "ink-docstrap": "^1.3.0",
    "jest": "^20.0.4",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.3.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-formal": "^0.25.4",
    "react-scripts": "^1.0.10",
    "react-test-renderer": "^15.6.1",
    "semantic-ui-react": "^0.70.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-node-externals": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^15.5.0",
    "react-dom": "^15.6.0",
    "semantic-ui-react": "^0.70.0"
  },
  "dependencies": {
    "controls": "^0.1.15",
    "debug": "^2.6.8",
    "js-joda": "^1.5.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react-flow-types": "^0.1.1",
    "react-widgets": "^3.4.8",
    "yup": "^0.21.3"
  }
}
